{
    "manifest_version": 3,
    "name": "Alarm Clock Extension",
    "version": "1.0",
    "description": "Set alarms and get notified with sound at specified times.",
    "permissions": [
      "alarms",
      "notifications",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  